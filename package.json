    "eslint": "^8.26.0",
    "eslint-config-stylelint": "^17.0.0",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "remark-cli": "^11.0.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier-scss": "^0.0.1",
