{
  "name": "lrr",
  "version": "0.2.0",
  "dependencies": {
    "gridstack": "^7.1.0"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "^2.0.0",
    "@stylelint/remark-preset": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^4.6.2",
    "browser-sync": "^2.27.10",
    "cssnano": "^5.1.14",
    "eslint": "^8.26.0",
    "eslint-config-stylelint": "^17.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-uglify-es": "^3.0.0",
    "jest": "^29.2.2",
    "jquery": "^3.6.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pixrem": "^5.0.0",
    "popper.js": "1.16.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "remark-cli": "^11.0.0",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "y18n": "^5.0.8"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,md,yml}": "prettier --write"
  },
  "prettier": "@stylelint/prettier-config",
  "eslintConfig": {
    "extends": [
      "stylelint"
    ],
    "globals": {
      "module": true,
      "require": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "overrides": {
    "graceful-fs": "^4.2.9"
  },
  "resolutions": {
    "graceful-fs": "^4.2.9"
  },
  "engines": {
    "node": ">=8"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "dev": "gulp",
    "build": "gulp generate-assets",
    "format": "prettier . --write",
    "lint:formatting": "prettier . --check",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*"
  }
}
